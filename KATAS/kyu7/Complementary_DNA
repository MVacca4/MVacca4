public class DnaStrand {
  public static String makeComplement(String dna) {
    char[] ch = dna.toCharArray();
    String newString = "";

    for (char c : ch) {
      switch (c) {
        case 'A':
          newString += 'T';
          break;
        case 'T':
          newString += 'A';
          break;
        case 'C':
          newString += 'G';
          break;
        case 'G':
          newString += 'C';
          break;
        default:
      }
    }
    return newString.toString();
  }
}
